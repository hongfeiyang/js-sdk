{
  "name": "@meeco/sdk",
  "description": "A wrapper library around Meeco's various API SDK's and encryption libraries",
  "version": "0.0.11",
  "author": "Meeco",
  "bugs": "https://github.com/Meeco/cli/issues",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "@meeco/cryppo": "^0.6.0",
    "@meeco/keystore-api-sdk": "0.13.1-beta.0",
    "@meeco/vault-api-sdk": "0.18.1-beta.0",
    "@types/node-forge": "~0.9.0",
    "base-x": "~3.0.7",
    "chalk": "^4.0.0",
    "debug": "^4.1.1",
    "form-data": "^3.0.0",
    "jimp": "^0.12.1",
    "jsrp": "~0.2.4",
    "node-fetch": "^2.6.0",
    "node-forge": "0.8.5",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@oclif/test": "^1.2.6",
    "@storybook/addon-info": "^5.3.19",
    "@storybook/addon-notes": "^5.3.19",
    "@storybook/html": "^5.3.19",
    "@storybook/preset-typescript": "^3.0.0",
    "@types/base-x": "^3.0.6",
    "@types/chai": "~4.2.7",
    "@types/debug": "^4.1.5",
    "@types/express": "~4.17.2",
    "@types/jsrp": "~0.2.1",
    "@types/mocha": "~5.2.7",
    "@types/node": "^10.14.18",
    "@types/sinon": "~7.5.1",
    "@types/yaml": "~1.2.0",
    "babel-loader": "^8.1.0",
    "bili": "^4.10.0",
    "chai": "~4.2.0",
    "clean-webpack-plugin": "^3.0.0",
    "lerna": "^3.22.0",
    "mocha": "~7.0.0",
    "nock": "~11.7.2",
    "nyc": "~15.0.0",
    "prettier": "~1.19.1",
    "progress-bar-webpack-plugin": "^2.1.0",
    "rollup-plugin-typescript2": "^0.27.1",
    "sass": "^1.26.8",
    "sass-loader": "^9.0.2",
    "sinon": "~8.1.1",
    "terser-webpack-plugin": "^3.0.6",
    "ts-loader": "^8.0.0",
    "ts-mock-imports": "^1.3.0",
    "ts-node": "^8.1.0",
    "tsconfig-paths": "^3.9.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^5.16.0",
    "typedoc": "0.17.0-3",
    "typescript": "^3.9.6",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "engines": {
    "node": ">=12.4.0"
  },
  "files": [
    "/dist"
  ],
  "homepage": "https://github.com/Meeco/cli",
  "keywords": [
    "meeco",
    "sdk",
    "meeco sdk"
  ],
  "license": "MIT",
  "repository": "Meeco/cli",
  "scripts": {
    "build": "tsc -p tsconfig.build.json && bili src/index.ts --format cjs --format es --module-name meeco-sdk",
    "demo": "webpack-dev-server demo",
    "format:check": "prettier -c \"{src,test,stories}/**/*.{ts,html,json,js,css,scss}\" *.json README.md",
    "format:write": "prettier --write \"{src,test,stories}/**/*.{ts,html,json,js,css,scss}\" *.json README.md",
    "lint": "tslint -p . -t stylish -c ../../tslint.json && tslint -p test/tsconfig.json -t stylish -c ../../tslint.json",
    "posttest": "npm run lint && npm run format:check",
    "prepack": "npm run build",
    "start": "npm run demo",
    "build:storybook": "build-storybook -c .storybook -o ../../docs/sdk",
    "build:typedoc": "typedoc --inputFiles src/index.ts --mode library --out ../../docs --tsconfig tsconfig.json --ignoreCompilerErrors --disableOutputCheck --name 'Meeco SDK Documentation' --toc AuthData,ConnectionService,EncryptionKey,Environment,ItemService,SecretService,ShareService,TemplatesService,UserService --excludePrivate",
    "build:demo": "webpack --config webpack.config.prod.js",
    "docs": "npm run build:typedoc && npm run build:storybook && npm run build:demo",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "storybook": "start-storybook -p 6006"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
